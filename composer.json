{
    "name": "ogestor/laravel-boleto",
    "description": "Biblioteca com boletos para o laravel",
    "keywords": [
        "ogestor",
        "laravel-boleto"
    ],
    "homepage": "https://github.com/ogestor/laravel-boleto",
    "license": "MIT",
    "authors": [
        {
            "name": "oGestor Sistemas para sua empresa",
            "email": "desenvolvimento@ogestor.com.br"
        }
    ],
    "require": {
        "php": ">=5.5.0,<8.0",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "laravel/framework": "^5.0||^6.0",
        "neitanod/forceutf8": "^2.0",
        "setasign/fpdf": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.0,<9.0",
        "orchestra/testbench": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Eduardokum\\LaravelBoleto\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eduardokum\\LaravelBoleto\\Tests\\": "tests"
        }
    },
    "config": {
        "platform": {
            "php": "7.2.0"
        }
    }
}
